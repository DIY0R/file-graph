  "author": "Sobirov Diyorbek <sabirovdiyorkind@gmail.com>",
  "license": "MIT",
  "description": "This is a system for storing graphs in files and making queries to them",
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@types/node": "^22.1.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0"
  },
  "overrides": {
    "eslint": "^9.8.0"
  },
  "repository": {
    "type": "git",
